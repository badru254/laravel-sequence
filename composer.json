{
    "name": "badru254/laravel-sequence",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Grabania",
            "email": "info@nextgen-tech.pl"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "laravel/framework": "^10.10",
        "bensampo/laravel-enum": "^6.8"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.87",
        "nunomaduro/larastan": "^0.7.6",
        "orchestra/testbench": "^6.18",
        "psy/psysh": "^0.10.8",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "NGT\\Laravel\\Sequence\\": "src/",
            "NGT\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NGT\\Laravel\\Sequence\\SequenceServiceProvider"
            ]
        }
    }
}
